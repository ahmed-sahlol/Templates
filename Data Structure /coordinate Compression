struct coordinateCopmression {
private:
    vector<int> init;

    void compress(vector<int> &v) {
        sort(v.begin(), v.end());
        v.erase(unique(v.begin(), v.end()), v.end());
    }

public:
    coordinateCopmression(vector<int> &v) {
        init = v;
        compress(init);
    }

    int index(int val) {
        return lower_bound(init.begin(), init.end(), val) - init.begin();
    }

    int initVal(int idx) {
        return init[idx];
    }
};
